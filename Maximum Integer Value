class Solution {
  public:
    long long int solve(int n,string &s)
     {   if(n==1)
          return s[0]-'0';
         long long int x=solve(n-1,s);
         return max(x+(s[n-1]-'0'),x*(s[n-1]-'0'));
     }
    long long int MaximumIntegerValue(string &s) {
        return solve(s.size(),s);
    }
};
