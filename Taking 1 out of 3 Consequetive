class Solution{
    int n;
    public:
    int fun(int idx,int *a,vector<int>&t)
     {  if(n-2<=idx)
          return 0;
        if(t[idx]!=-1)
         return t[idx];
        return t[idx]=min(a[idx]+fun(idx+1,a,t),min(a[idx+1]+fun(idx+2,a,t),a[idx+2]+fun(idx+3,a,t)));
     }
    int minSum(int a[], int n)
    {   this->n=n;
        vector<int>t(n,-1);
        return fun(0,a,t);
    }
};
