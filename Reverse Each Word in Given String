class Solution
{
  public:
    string reverseWords (string s)
    { string res;
      stack<char>st;
      int i=0,n=s.size();
      while(i<s.size())
      {  while(i<n && s[i]!='.')
           st.push(s[i++]);
         while(st.empty()==false)
         {  res.push_back(st.top());
            st.pop();
         }
         if(i!=n)
           res.push_back('.');
         ++i;
      }
      return res;
    }
};
