class Solution {
  public:
    int firstElement(int n) // Find pattern relation for a^2 , a^3 , a^4 , a^5 . Afer finding pattern we can easily find a^(n)
    { if(n==1 || n==2)
         return 1;
      int a=1,b=2,temp;
      for(int k=3;k<=n;++k)
      {  temp=(a+b)%(1000000007);
         a=b;
         b=temp;
      }
      return a;
    }
};
